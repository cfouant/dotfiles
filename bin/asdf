#!/usr/bin/env python

# fills your terminal screen with gibberish text

import sys
import random
import time

char_range = [32, 126]

random.seed()
try:

    while True:
        sys.stdout.write("%s" % chr(random.randint(*char_range)))
        sys.stdout.flush()
        time.sleep(0.0001)

except KeyboardInterrupt:
    exit()
